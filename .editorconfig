# Editor configuration, see https://editorconfig.org
root = true

# Unix-style newlines with a newline ending every file
[*]
end_of_line = lf
insert_final_newline = true
trim_trailing_whitespace = true
charset = utf-8
indent_style = tab

[*.yml]
indent_style = space
indent_size = 2

[*.md]
max_line_length = off
trim_trailing_whitespace = false

[*.diff]
trim_trailing_whitespace = false

[package.json]
indent_style = space

[.ebextensions/*]
indent_style = space
